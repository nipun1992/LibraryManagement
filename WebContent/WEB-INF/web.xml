<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:web="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>LibraryProject</display-name>
	<welcome-file-list>
		<welcome-file>FrontPage.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>createUser</servlet-name>
		<servlet-class>com.User.CreateUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>createUser</servlet-name>
		<url-pattern>/user</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>createLibrarian</servlet-name>
		<servlet-class>com.Librarian.VerifyLibrarian</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>createLibrarian</servlet-name>
		<url-pattern>/Librarian</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>insertbook</servlet-name>
		<servlet-class>com.Librarian.AddBook</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>insertbook</servlet-name>
		<url-pattern>/addbook</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>existUser</servlet-name>
		<servlet-class>com.User.ExistingUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>existUser</servlet-name>
		<url-pattern>/exstusr</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>issuebook</servlet-name>
		<servlet-class>com.User.BookIssue</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>issuebook</servlet-name>
		<url-pattern>/issuebk</url-pattern>
	</servlet-mapping>
</web-app>